{
  "name": "remotion-renderer",
  "version": "1.0.0",
  "description": "Remotion-based video renderer for InsightLearn burned-in subtitles",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && remotion bundle src/compositions/index.ts --out-dir dist/bundle",
    "start": "node dist/index.js",
    "render": "remotion render src/compositions/index.ts VideoWithCaptions out.mp4",
    "studio": "remotion studio src/compositions/index.ts"
  },
  "dependencies": {
    "remotion": "^4.0.416",
    "@remotion/cli": "^4.0.421",
    "@remotion/captions": "^4.0.416",
    "@remotion/renderer": "^4.0.416",
    "@remotion/bundler": "^4.0.421",
    "express": "^4.21.0",
    "mongodb": "^6.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.9.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.6.3",
    "tsx": "^4.19.2",
    "@types/react": "^18.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
