            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "expr": "label_replace(node_api_requests_by_country, \"country\", \"United States\", \"country\", \"United States of America\")",
            "format": "table",
            "instant": true,
            "legendFormat": "__auto",
            "refId": "A"
          }
        ],
        "title": "üåç Requests by Country",
        "transformations": [
          {
            "id": "labelsToFields",
            "options": {
              "keepLabels": [
                "country"
              ],
              "mode": "columns"
            }
          },
          {
