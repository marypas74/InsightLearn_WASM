        "id": 24,
        "options": {
          "cellHeight": "sm",
          "footer": {
            "reducer": [
              "sum"
            ],
            "show": true
          },
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Total"
            }
          ]
        },
        "targets": [
          {
            "expr": "node_filesystem_size_bytes{mountpoint=~\"/|/home|/boot|/k3s-zfs\"} / 1024 / 1024 / 1024",
            "format": "table",
            "instant": true,
            "refId": "Total"
          },
          {
            "expr": "(node_filesystem_size_bytes{mountpoint=~\"/|/home|/boot|/k3s-zfs\"} - node_filesystem_avail_bytes{mountpoint=~\"/|/home|/boot|/k3s-zfs\"}) / 1024 / 1024 / 1024",
            "format": "table",
            "instant": true,
            "refId": "Used"
          },
          {
            "expr": "node_filesystem_avail_bytes{mountpoint=~\"/|/home|/boot|/k3s-zfs\"} / 1024 / 1024 / 1024",
            "format": "table",
            "instant": true,
            "refId": "Available"
          }
        ],
        "title": "üìä Disk Space Details (GB)",
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "device": true,
                "fstype": true,
                "instance": true,
                "job": true
              },
              "indexByName": {
                "mountpoint": 0
              },
              "renameByName": {
                "Value #Available": "Available",
                "Value #Total": "Total",
                "Value #Used": "Used",
                "mountpoint": "Partition"
              }
            }
          },
          {
            "id": "sortBy",
            "options": {
              "fields": {},
              "sort": [
                {
                  "desc": true,
                  "field": "Total"
                }
              ]
            }
          }
        ],
        "type": "table"
