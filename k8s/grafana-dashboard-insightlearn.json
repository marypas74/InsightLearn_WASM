                    "horizontal": "center",
                    "vertical": "center"
                  },
                  "text": {
                    "field": "country",
                    "fixed": "",
                    "mode": "field"
                  },
                  "textConfig": {
                    "fontSize": 10,
                    "offsetX": 0,
                    "offsetY": 12,
                    "textAlign": "center",
                    "textBaseline": "top"
                  }
                }
              },
              "location": {
                "gazetteer": "public/gazetteer/countries.json",
                "lookup": "country",
                "mode": "lookup"
              },
              "name": "Requests by Country",
              "tooltip": true,
              "type": "markers"
